{
  "name": "FrickDMCA",
  "version": "0.0.0",
  "scripts": {
    "build": "rollup -c && mkdir -p build && inline-assets --htmlmin dist/index.html build/index.html",
    "watch": "rollup -c -w",
    "dev": "rollup -c -w",
    "clean": "cargo clean && rimraf dist",
    "check-rs": "cargo check --target=wasm32-unknown-unknown"
  },
  "dependencies": {},
  "devDependencies": {
    "@rollup/plugin-html": "^0.2.0",
    "@wasm-tool/rollup-plugin-rust": "^1.0.5",
    "inline-assets": "^1.4.7",
    "rimraf": "^3.0.2",
    "rollup": "^2.34.2",
    "rollup-plugin-serve": "^1.1.0",
    "rollup-plugin-styles": "^3.11.0",
    "rollup-plugin-terser": "^7.0.2"
  }
}
